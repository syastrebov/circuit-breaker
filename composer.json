{
  "name": "syastrebov/circuit-breaker",
  "description": "Circuit breaker wrapper for microservices and api calls.",
  "authors": [
    {
      "name": "Sergey Yastrebov",
      "email": "serg.yastrebov@gmail.com",
      "homepage": "https://github.com/syastrebov"
    }
