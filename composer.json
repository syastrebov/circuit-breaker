  "require-dev": {
    "ext-memcached": "*",
    "ext-pdo": "*",
    "ext-redis": "*",
    "phpunit/phpunit": "^12.5",
    "phpstan/phpstan": "^2.1",
    "squizlabs/php_codesniffer": "^4.0",
    "vimeo/psalm": "^6.15"
  },
  "autoload": {
    "psr-4": {
      "": "src/",
      "Tests\\": "tests/"
    }
  }
